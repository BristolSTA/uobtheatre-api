[run]
branch = True
omit = *migrations*,
       *urls*,
       *test*,
       *admin*,
       ./manage.py,
       ./uobtheatre-api/config/*,
       ./uobtheatre-api/wsgi.py,
       *__init__*
       ./uobtheatre/management/*

[report]
exclude_lines =
    pragma: no cover
    raise NotImplementedError
    if TYPE_CHECKING
